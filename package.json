{
	"name": "zenml-dashboard",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc && vite build",
		"dev": "vite",
		"format": "prettier  --write .",
		"generate:types": "node scripts/types.js",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"prepare": "husky",
		"preview": "vite preview",
		"security-refresh": "rm pnpm-lock.yaml && pnpm install && pnpm audit",
		"test:e2e": "playwright test",
		"test:unit": "vitest run"
	},
	"lint-staged": {
		"*.{cjs,js,jsx,ts,tsx,css,scss}": [
			"prettier --write"
		],
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix"
		]
	},
	"dependencies": {
		"@fontsource/inter": "^5.2.5",
		"@hookform/resolvers": "^3.10.0",
		"@radix-ui/react-tabs": "^1.1.3",
		"@tanstack/react-query": "^5.72.0",
		"@tanstack/react-table": "^8.21.2",
		"@zenml-io/react-component-library": "^0.23.1",
		"awesome-debounce-promise": "^2.1.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"elkjs": "^0.10.0",
		"featureos-widget": "^0.0.32",
		"immer": "^10.1.1",
		"jwt-decode": "^4.0.0",
		"lodash.debounce": "^4.0.8",
		"papaparse": "^5.5.2",
		"prismjs": "^1.30.0",
		"react": "^18.3.1",
		"react-countdown": "^2.3.6",
		"react-dom": "^18.3.1",
		"react-error-boundary": "^5.0.0",
		"react-hook-form": "^7.55.0",
		"react-joyride": "^2.9.3",
		"react-markdown": "^9.0.3",
		"react-router-dom": "^6.27.0",
		"reactflow": "^11.11.4",
		"reodotdev": "^1.0.0",
		"unique-names-generator": "^4.7.1",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@playwright/test": "^1.51.1",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@types/lodash.debounce": "^4.0.9",
		"@types/node": "^22.14.0",
		"@types/papaparse": "^5.3.15",
		"@types/prismjs": "^1.26.5",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/eslint-plugin": "^7.12.0",
		"@typescript-eslint/parser": "^7.12.0",
		"@vitejs/plugin-react-swc": "^3.8.1",
		"autoprefixer": "^10.4.21",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.19",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.0",
		"openapi-typescript": "^6.7.6",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.3",
		"vite": "^6.2.7",
		"vite-plugin-bundle-prefetch": "^0.0.4",
		"vite-plugin-svgr": "^4.3.0",
		"vitest": "^3.1.1"
	},
	"pnpm": {
		"overrides": {
			"@babel/helpers": "^7.26.10",
			"@babel/runtime": "^7.26.10",
			"braces": "^3.0.3",
			"cross-spawn": "^7.0.5",
			"micromatch": "^4.0.8",
			"nanoid": "^3.3.8",
			"undici": "^5.29.0"
		}
	}
}
